{
  "schemaVersion": 1,
  "id": "elytra_aeronautics",
  "version": "${version}",

  "name": "Elytra aeronautics",
  "description": "Fly faster with the elytra the higher you travel and customise cloud layers at will! \nThis mod is highly configurable, use the '/ean' command without extra arguments for more information. \nFor an in-depth explanation regarding configuration, click the Website button above.",
  "authors": [
    "Soulphur"
  ],
  "contact": {
    "homepage": "https://github.com/Soulphur0/ElytraAeronautics",
    "sources": "https://github.com/Soulphur0/ElytraAeronautics",
    "issues": "https://github.com/Soulphur0/ElytraAeronautics/issues"
  },

  "license": "MIT",
  "icon": "assets/elytra_aeronautics/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.Soulphur0.ElytraAeronautics"
    ],
    "preLaunch": [
      "com.llamalad7.mixinextras.MixinExtrasBootstrap::init"
    ]
  },
  "mixins": [
    "elytraaeronautics.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "minecraft": "1.19.x",
    "java": ">=17"
  },
  "suggests": {
    "clothconfig": ">=0.6.0",
    "modmenu": ">=3.0.1"
  }
}
